<Application x:Class="MVVM_First.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MVVM_First"
             xmlns:ViewModel="clr-namespace:MVVM_First.ViewModel"
             xmlns:View="clr-namespace:MVVM_First.View"
             StartupUri="/View/ConverterView.xaml">
    
    
    <Application.Resources>
        <DataTemplate DataType="{x:Type ViewModel:ConverterViewModel}">
            <View:ConverterView></View:ConverterView>
        </DataTemplate>
    </Application.Resources>
    
</Application>
